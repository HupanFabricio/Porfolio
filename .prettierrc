{
    "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
    "semi": true,
    "singleQuote": true,
    "singleAttributePerLine": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ]
}
